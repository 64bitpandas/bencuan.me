---
import ColorLayout from '../../components/color-app/ColorLayout.astro';
import ExplorerView from '../../components/color-app/ExplorerView';
import pantoneNames from '../../color-books/_pantone-color-names.json';
import favoritesPreset from '../../color-books/_favorites.json';

// Import all color book JSONs
import anpa from '../../color-books/ANPA Color.json';
import dic from '../../color-books/DIC Color Guide.json';
import focoltone from '../../color-books/FOCOLTONE.json';
import hksEProcess from '../../color-books/HKS E Process.json';
import hksE from '../../color-books/HKS E.json';
import hksKProcess from '../../color-books/HKS K Process.json';
import hksK from '../../color-books/HKS K.json';
import hksNProcess from '../../color-books/HKS N Process.json';
import hksN from '../../color-books/HKS N.json';
import hksZProcess from '../../color-books/HKS Z Process.json';
import hksZ from '../../color-books/HKS Z.json';
import pantoneFHCotton from '../../color-books/PANTONE F+H cotton TCX.json';
import pantoneFHNylon from '../../color-books/PANTONE F+H nylon brights TN.json';
import pantoneFHI from '../../color-books/PANTONE FHI Color Guide.json';
import pantoneGoeCoated from '../../color-books/PANTONE Goe coated.json';
import pantoneGoeUncoated from '../../color-books/PANTONE Goe uncoated.json';
import pantoneGoeBridge from '../../color-books/PANTONE GoeBridge coated.json';
import pantoneSkinTone from '../../color-books/PANTONE SkinTone Guide.json';
import pantonePlusCMYKCoated from '../../color-books/PANTONE+ CMYK Coated.json';
import pantonePlusCMYKUncoated from '../../color-books/PANTONE+ CMYK Uncoated.json';
import pantonePlusColorBridgeCoated from '../../color-books/PANTONE+ Color Bridge Coated.json';
import pantonePlusColorBridgeUncoated from '../../color-books/PANTONE+ Color Bridge Uncoated.json';
import pantonePlusExtendedGamut from '../../color-books/PANTONE+ Extended Gamut Coated.json';
import pantonePlusMetallics from '../../color-books/PANTONE+ Metallics Coated.json';
import pantonePlusPastelsCoated from '../../color-books/PANTONE+ Pastels & Neons Coated.json';
import pantonePlusPastelsUncoated from '../../color-books/PANTONE+ Pastels & Neons Uncoated.json';
import pantonePlusPremiumMetallics from '../../color-books/PANTONE+ Premium Metallics Coated.json';
import pantonePlusSolidCoated from '../../color-books/PANTONE+ Solid Coated.json';
import pantonePlusSolidUncoated from '../../color-books/PANTONE+ Solid Uncoated.json';
import toyo94 from '../../color-books/TOYO 94 COLOR FINDER.json';
import toyoColorFinder from '../../color-books/TOYO COLOR FINDER.json';
import trumatch from '../../color-books/TRUMATCH.json';

const colorBooks = [
  anpa,
  dic,
  focoltone,
  hksE,
  hksEProcess,
  hksK,
  hksKProcess,
  hksN,
  hksNProcess,
  hksZ,
  hksZProcess,
  pantoneFHCotton,
  pantoneFHNylon,
  pantoneFHI,
  pantoneGoeCoated,
  pantoneGoeUncoated,
  pantoneGoeBridge,
  pantoneSkinTone,
  pantonePlusCMYKCoated,
  pantonePlusCMYKUncoated,
  pantonePlusColorBridgeCoated,
  pantonePlusColorBridgeUncoated,
  pantonePlusExtendedGamut,
  pantonePlusMetallics,
  pantonePlusPastelsCoated,
  pantonePlusPastelsUncoated,
  pantonePlusPremiumMetallics,
  pantonePlusSolidCoated,
  pantonePlusSolidUncoated,
  toyo94,
  toyoColorFinder,
  trumatch,
];
---

<ColorLayout title="Explorer | Ben's Colors" currentPage="explorer">
  <ExplorerView 
    colorBooks={colorBooks} 
    pantoneNames={pantoneNames} 
    favoritesPreset={favoritesPreset}
    client:load 
  />
</ColorLayout>

